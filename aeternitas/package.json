{
    "name": "aeternitas",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --watch --noEmit",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
        "dev": "concurrently --raw --group \"tsx watch ./src/index.ts\" \"pnpm run typecheck\" \"pnpm run lint\""
    },
    "dependencies": {
        "@prisma/client": "^4.16.2",
        "bcrypt": "^5.1.0",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.1",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "concurrently": "^8.2.0",
        "eslint": "^8.44.0",
        "prisma": "^4.16.2",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6"
    }
}
