{
    "name": "aeternitas",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "build": "cross-env NODE_ENV=production tsc",
        "typecheck": "tsc --watch --noEmit",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
        "migrate:dev": "prisma migrate dev",
        "migrate:reset": "prisma migrate reset",
        "migrate:deploy": "primsa migrate deploy",
        "studio": "prisma studio",
        "dev": "concurrently --raw --group \"cross-env NODE_ENV=development tsx watch ./src/index.ts\" \"pnpm run typecheck\" \"pnpm run lint\""
    },
    "dependencies": {
        "@prisma/client": "^4.16.2",
        "bcrypt": "^5.1.0",
        "connect-redis": "^7.1.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "ioredis": "^5.3.2",
        "node-fetch": "^3.3.1",
        "socket.io": "^4.7.1",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/node": "^20.4.1",
        "@types/node-fetch": "^2.6.4",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "concurrently": "^8.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.44.0",
        "prisma": "^4.16.2",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6"
    }
}
